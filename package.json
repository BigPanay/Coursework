  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "format": "prettier --write \"server/**/*.ts\"",
    "webpack-dev": "rm -rf dist && NODE_ENV=development webpack --config config/webpack.config.dev.js",
    "run-app": "cd dist && node server",
    "webpack-prod": "rm -rf dist && NODE_ENV=production webpack --config config/webpack.config.prod.js && cp .env dist/",
    "microservice": "rm -rf server-queue/.env && cp .env server-queue && NODE_ENV=development && npm start",
    "start": "ts-node -r tsconfig-paths/register server/main.ts",
    "start:dev": "concurrently --handle-input \"wait-on dist/main.js && nodemon\" \"tsc -w -p tsconfig.build.json\" ",
    "start:debug": "nodemon --config nodemon-debug.json",
    "prestart:prod": "rimraf dist && npm run build",
    "start:prod": "node dist/main.js",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "update:packages": "rm -rf node_modules && npm update --save-dev && npm update --save"
  },
